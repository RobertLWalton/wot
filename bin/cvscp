#! /bin/csh -fe
set root=`cat CVS/Root`
set account=`expr "$root" : '\([^:]*\):' `
set root=`expr "$root" : '[^:]*:\(.*\)$' `
set repository="$root/`cat CVS/Repository`"
echo ssh "$account" cp -p "$repository/$1,v" "$repository/$2,v"
ssh "$account" cp -p "$repository/$1,v" "$repository/$2,v"
echo cvs update "$2"
cvs update "$2"
echo chmod u+w "$2"
chmod u+w "$2"
